package com.ecity.mobile.android.crossanalysis;

import java.io.Serializable;

/**
 * @author wangfeng 管线颜色配置基类对象
 * 
 */
public class PipeColorBean implements Serializable {

    /**
     * 
     */
    private static final long serialVersionUID = 9187113435846669484L;
    private String code;
    private String name;
    private int color;
    private String pipeAltitudeType;

    public PipeColorBean() {

    }

    public PipeColorBean(String code, String name, int color,
            String pipeAltitudeType) {
        super();
        this.code = code;
        this.name = name;
        this.color = color;
        this.pipeAltitudeType = pipeAltitudeType;
    }

    public String getCode() {
        return code;
    }

    public void setCode(String code) {
        this.code = code;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public int getColor() {
        return color;
    }

    public void setColor(int color) {
        this.color = color;
    }

    public String getPipeAltitudeType() {
        return pipeAltitudeType;
    }

    public void setPipeAltitudeType(String pipeAltitudeType) {
        this.pipeAltitudeType = pipeAltitudeType;
    }

    @Override
    public String toString() {
        return "PipeColorBean [code=" + code + ", name=" + name + ", color=" + color
                + ", pipeAltitudeType=" + pipeAltitudeType + "]";
    }
}
