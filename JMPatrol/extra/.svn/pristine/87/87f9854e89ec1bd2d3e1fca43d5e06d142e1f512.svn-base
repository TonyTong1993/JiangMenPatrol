<?xml version="1.0" encoding="UTF-8"?><LinearLayout     android:layout_width="fill_parent"    android:layout_height="fill_parent"    android:background="@color/transparent"     xmlns:android="http://schemas.android.com/apk/res/android">     <RelativeLayout         android:id="@+id/spinner_dropdown_layout"         android:layout_width="fill_parent"         android:layout_height="@dimen/margin_36"         android:background="@drawable/css_customspinner_listitem"         android:gravity="left|center" >         <TextView             android:id="@+id/spinner_dropdown_txt"             android:layout_width="fill_parent"             android:layout_height="wrap_content"             android:layout_alignParentLeft="true"             android:layout_centerVertical="true"             android:textSize="@dimen/font_18" />    </RelativeLayout></LinearLayout>